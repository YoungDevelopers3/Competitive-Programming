#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
        int n=0,d=0;
        long long int prod=1;
		cin>>n;
        int a[n];
		for(int i=0;i<n;i++){
			cin>>a[i];
			prod=prod*a[i];
      
		}
        d=prod%10;
		if(d==2||d==3||d==5){
			cout<<"YES"<<"\n";
		}
		else{
			cout<<"NO"<<"\n";
		}
		

	}

}
